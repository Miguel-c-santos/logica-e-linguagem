listanumeros = ("zero", "um", "dois", "trÃªs", "quatro", "cinco", "seis", "sete", "oito", "nove","dez", "onze", "doze", "treze", "quatorze", "quinze", "dezesseis", "dezessete", "dezoito", "dezenove", "vinte")

while True:
    num = int(listanumeros.count(input("Insira um valor desejado de 0 a 20: ")))
    if num < 0 or num > 20:
        break

    print(len(listanumeros[num]))
